<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping package="com.xmszit.voip.client.entity">

    <class name="UserRole" table="CT_USER_ROLE">
        <id name="id" column="ID">
            <generator class="uuid2"/>
        </id>
        <property name="clientId" column="CLIENT_ID"></property>
        <property name="roleName" column="ROLE_NAME"/>
        <property name="powerContent" column="POWER_CONTENT"/>
        <property name="remark" column="REMARK"/>
        <property name="deleted" column="DELETED"/>
      	<property name="adminRole" column="ADMIN_ROLE"/>
     
    </class>

</hibernate-mapping>