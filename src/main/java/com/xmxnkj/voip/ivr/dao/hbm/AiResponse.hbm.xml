<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping package="com.xmxnkj.voip.ivr.entity">

    <class name="AiResponse"  table="AI_RESPONSE">
    
        <id name="id" column="ID">
            <generator class="uuid2"/>
        </id>
              	
      	<property name="nextStepCode" column="NEXT_STEP_CODE"/>
      	
      	<property name="answer" column="ANSWER"/>
      	
       <property name="sound" column="SOUND"/>
      	
      	<property name="remainOriginal" column="REMAIN_ORIGINAL"/>
	
		<property name="remainOriginalAnswer" column="REMAIN_ORIGINAL_ANSWER"/>
		
		<property name="remainOriginalSound" column="REMAIN_ORIGINAL_SOUND"/>
		
		<property name="code" column="CODE"/>
		
	    <property name="repeat" column="IS_REPEAT"/>
		
		<property name="flowId" column="FLOW_ID"/>
		
		<property name="keywordsString" column="KEYWORDS_STRING"/>

      	<property name="deleted" column="DELETED"/>
      	
      	<property name="clientId" column="CLIENT_ID"/>
      	
      	<property name="templateId" column="TEMPLATE_ID"/>
		<property name="flowCode" column="FLOW_CODE"/>
		
		<property name="blockAsr" column="BLOCK_ASR"/>
        
    </class>
</hibernate-mapping>